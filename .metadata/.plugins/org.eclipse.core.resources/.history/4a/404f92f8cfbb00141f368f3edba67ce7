
public class BinarySearchTrees 
{
	
	static BST m_objBST = new BST();
	
	public static void main(String[] args) 
	{
		m_objBST.Insert(10);
		m_objBST.Insert(50);
		m_objBST.Insert(20);
		m_objBST.Insert(10000);
		
	//	m_objBST.print_inorder(m_objBST.m_objRootNode);
		m_objBST.Delete(20);
		m_objBST.print_inorder(m_objBST.m_objRootNode);
	}
	
}
